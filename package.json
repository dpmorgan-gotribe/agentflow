{
  "name": "agentflow",
  "version": "2.0.0",
  "type": "module",
  "bin": {
    "agentflow": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc && npm run copy-templates",
    "copy-templates": "node -e \"const fs=require('fs');fs.cpSync('src/templates','dist/templates',{recursive:true})\"",
    "dev": "tsc --watch"
  },
  "dependencies": {
    "commander": "^12.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  }
}
